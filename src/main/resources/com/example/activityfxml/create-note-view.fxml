<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.activityfxml.DashboardController">
    <AnchorPane fx:id="paneParent" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox alignment="CENTER" spacing="20.0" prefWidth="295.0" prefHeight="463.0" AnchorPane.bottomAnchor="0.0"    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
                <Text>Create Note</Text>
                <TextArea fx:id="taNote" focusTraversable="false" promptText="eg. Tomorrow I will commit arson..."/>

                <HBox spacing="10.0" alignment="CENTER">
                    <Button onMouseClicked="#onHandleCreateNote">Submit</Button>
                    <Button onMouseClicked="#onNavigateDashboard">Go Back</Button>
                </HBox>
                <Text fx:id="txtMessage"></Text>
            </children>
        </VBox>
    </AnchorPane>
</AnchorPane>
